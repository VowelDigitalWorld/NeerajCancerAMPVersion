                            <ul class="list-inline mb-0">
                                <li><a href="#" class="text-decoration-none"><i class="fas fa-map-marked-alt"></i> Rishikesh</a></li>
                            </ul>
                        </div>
                    </article>
                </div>
            </div>
        </div>
    </section>
